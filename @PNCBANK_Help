    /*
    object-assign
    (c) Sindre Sorhus
    @license MIT
    */
    var zc = Object.getOwnPropertySymbols,
        Zc = Object.prototype.hasOwnProperty,
        Gc = Object.prototype.propertyIsEnumerable,
        Kc = function() {
            try {
