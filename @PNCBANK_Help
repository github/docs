    function go(n, t) {
        var e = gi(t[Ja]),
            r = so(bi(t[Da])),
            i = ho(r),
            o = void 0;
        try {
            o = qn(n(e, r))
        } catch (n) {
            return Un(mo(t, n))
        }
        return T(i) ? o.then(function() {
            return vo(t)
        })['catch'](function(n) {
            return mo(t, n)
        }) : o.then(function() {
            return po(i)
        }).then(function() {
            return vo(t)
        })['catch'](function(n) {
            return mo(t, n)
        })
    }

    function yo(n, t) {
