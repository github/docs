<script>
	// Message
	var alertText = 'ALERT: Phone, Text and E-Mail Campaign Targeting PNC Customers <a href="https://www.pnc.com/en/customer-service/update-center.html">Learn How to Protect Yourself &raquo;</a>';

	jQuery(document).ready(function() {
		var alertHTML = '<div class="pnc-alert--container"><div class="pnc-alert--msg"><p><span class="pnc-alert--icon"><i class="fa fa-exclamation-triangle" aria-hidden="true"></i></span><span class="visually-hidden">Alert:&nbsp;</span>' + alertText + '</p></div></div>';
		var alertDom = document.createElement('div');
		alertDom.setAttribute("class","pnc-alert");
		alertDom.innerHTML = alertHTML;
		var targetNode = document.getElementById("mast-nav");
		if(targetNode) {
			targetNode.parentNode.insertBefore(alertDom, targetNode.nextSibling);
		}
	});
</script></div>
