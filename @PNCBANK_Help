<script>
    jQuery(document).ready(function(){
		var authoring;
        if (jq191.cookie("wcmmode") == "edit"){//CQ is only defined in authoring
            authoring = true;              
        } else {
			authoring = false;
        }

        	console.log(authoring);

        var mega = jQuery('.thoughtstarter-mega');
        		if (mega.length > 0){
				checkViewPort();
            		jQuery(window).on("resize",checkViewPort);
        			}

        function checkViewPort(){
            if (!authoring){
            		var viewportSize = window.innerWidth;
            		if (viewportSize >= 980){
					jQuery('.thoughtstarter-hero').hide();
            		} else if (viewportSize < 980 && viewportSize > 600) {
					jQuery('.thoughtstarter-hero').show();
            		} else if (viewportSize < 600){
			    	jQuery('.thoughtstarter-hero').hide();
            		}
            } else {//we're in authoring, don't do anything
				return false;
            }
        			}

    });
</script>
</section>


                
            
