        var google_custom_params = window.google_tag_params;
        var google_remarketing_only = true;
        document.write('<s' + 'cript type="text/javascript" src="//www.googleadservices.com/pagead/conversion.js"></s' + 'script>');
    }

    //gcbbk
    if (gcbbk) {
        var google_tag_params = {
            olbpr: olbpr,
            olbex: olbex
        };
        var google_conversion_id = 1031626219;
        var google_custom_params = window.google_tag_params;
        var google_remarketing_only = true;
        document.write('<s' + 'cript type="text/javascript" src="//www.googleadservices.com/pagead/conversion.js"></s' + 'cript>');
    }
 
</script>

