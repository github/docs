curl -X POST $ENDPOINT 
  -H "Content-Type: application/json" 
  -H "api-key: $API_KEY" 
  -d '{
      "messages": [
          {
              "role": "system",
              "content": "You are a helpful assistant."
          },
          {
              "role": "user",
              "content": "What is the capital of France?"
          }
      ],
      "temperature": 1.0,
      "top_p": 1.0,
      "max_tokens": 1000,
      "model": "$MODEL"
  }'
